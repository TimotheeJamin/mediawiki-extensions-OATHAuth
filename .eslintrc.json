{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery"
	],
	"globals": {
		"mw": false,
		"OO": false
	},
	"rules": {
		"array-bracket-spacing": 0,
		"camelcase": 0,
		"comma-spacing": 0,
		"computed-property-spacing": 0,
		"curly": 0,
		"eqeqeq": 0,
		"indent": 0,
		"key-spacing": 0,
		"keyword-spacing": 0,
		"linebreak-style": 0,
		"max-len": 0,
		"no-array-constructor": 0,
		"no-bitwise": 0,
		"no-implicit-globals": 0,
		"no-jquery/no-global-selector": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-multi-spaces": 0,
		"no-multiple-empty-lines": 0,
		"no-redeclare": 0,
		"no-tabs": 0,
		"no-trailing-spaces": 0,
		"no-undef": 0,
		"no-unused-vars": 0,
		"no-use-before-define": 0,
		"one-var": 0,
		"operator-linebreak": 0,
		"quotes": 0,
		"semi": 0,
		"space-before-blocks": 0,
		"space-before-function-paren": 0,
		"space-in-parens": 0,
		"space-infix-ops": 0,
		"spaced-comment": 0,
		"switch-colon-spacing": 0,
		"vars-on-top": 0,
		"wrap-iife": 0,
		"yoda": 0
	}
}
